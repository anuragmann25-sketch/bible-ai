{
  "expo": {
    "name": "Bible AI - Master Your Spirit",
    "slug": "bible-ai",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "bibleai",
    "splash": {
      "image": "./assets/splash-crucifix.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.despia.bibleai-theultimatewarrior",
      "buildNumber": "2"
    },
    "android": {
      "package": "com.despia.bibleai",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen"
    ]
  }
}
